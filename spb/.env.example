# Database Configuration
DB_URL=jdbc:h2:mem:testdb
DB_USERNAME=sa
DB_PASSWORD=
DB_DRIVER=org.h2.Driver

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRATION_TIME=86400000

# Kakao OAuth Configuration
KAKAO_LOGIN_URL=https://kauth.kakao.com/oauth/authorize
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_REDIRECT_URI=http://localhost:8080/kakao-authentication/login
KAKAO_TOKEN_REQUEST_URI=https://kauth.kakao.com/oauth/token
KAKAO_USER_INFO_REQUEST_URI=https://kapi.kakao.com/v2/user/me
KAKAO_CLIENT_SECRET=your-kakao-client-secret

# Google OAuth Configuration
GOOGLE_LOGIN_URL=https://accounts.google.com/o/oauth2/v2/auth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_REDIRECT_URI=http://localhost:8080/google-authentication/login
GOOGLE_TOKEN_REQUEST_URI=https://oauth2.googleapis.com/token
GOOGLE_USER_INFO_REQUEST_URI=https://www.googleapis.com/oauth2/v2/userinfo
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Naver OAuth Configuration
NAVER_LOGIN_URL=https://nid.naver.com/oauth2.0/authorize
NAVER_CLIENT_ID=your-naver-client-id
NAVER_REDIRECT_URI=http://localhost:8080/naver-authentication/login
NAVER_TOKEN_REQUEST_URI=https://nid.naver.com/oauth2.0/token
NAVER_USER_INFO_REQUEST_URI=https://openapi.naver.com/v1/nid/me
NAVER_CLIENT_SECRET=your-naver-client-secret